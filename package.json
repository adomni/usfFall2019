{
  "name": "usfFall2019",
  "version": "0.0.1",
  "description": "USF 2019 project",
  "main": "lambda.js",
  "engines": {
    "node": ">=10.13"
  },
  "scripts": {
    "compile": "yarn run clean && tsc && yarn run copy-static-files",
    "watch": "tsc -w .",
    "clean": "rm -Rf dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "lint": "node --max_old_space_size=32000 ./node_modules/tslint/bin/tslint --project .",
    "copy-static-files": "cd src && cp -R static ../dist",
    "generate-docs": "typedoc --out dist-doc",
    "run-kei": "yarn compile && node dist/service/student-runner/kei-runner",
    "run-tae": "yarn compile && node dist/service/student-runner/tae-runner",
    "run-tuo": "yarn compile && node dist/service/student-runner/tuo-runner"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@bitblit/ratchet": "0.4.2",

    "csv": "5.1.2",
    "injection-js": "2.2.2",
    "moment": "2.24.0",
    "moment-timezone": "0.5.26",
    "portable-fetch": "3.0.0",
    "reflect-metadata": "0.1.12",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "tmp": "0.1.0",
    "tunnel-ssh": "4.1.4"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/chai-http": "0.0.29",
    "@types/csv-parse": "1.1.12",
    "@types/debug": "0.0.29",
    "@types/mocha": "5.2.5",
    "@types/moment-timezone": "0.5.10",
    "@types/mysql2": "types/mysql2",
    "@types/node": "8.9.4",
    "aws-sdk": "2.517.0",
    "chai": "4.2.0",
    "chai-http": "4.2.1",
    "mocha": "5.2.0",
    "ssl-root-cas": "1.3.1",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "tslint": "5.11.0",
    "typedoc": "0.15.0",
    "typescript": "3.5.3"
  }
}
